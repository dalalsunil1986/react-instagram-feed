{
  "name": "react-redux-boilerplate",
  "version": "1.0.1",
  "private": true,
  "description": "A boilerplate of things that shouldn't exist",
  "main": "client/index.js",
  "scripts": {
    "test": "SET BABEL_DISABLE_CACHE=1 NODE_ENV=test && mocha client/**/*.test.js --compilers js:babel-register --recursive",
    "test:watch": "npm test -- --watch",
    "start": "SET NODE_ENV=development && webpack-dev-server -d --history-api-fallback --hot --inline --progress --colors --port 3000",
    "build": "webpack --progress --colors",
    "lint": "eslint client",
    "lint:fix": "eslint client --fix"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.3",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-plugin-webpack-loaders": "^0.7.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.5",
    "babel-runtime": "^6.5.0",
    "classnames": "^2.2.3",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "eslint-plugin-react": "^5.2.2",
    "file-loader": "^0.8.5",
    "font-awesome-loader": "^1.0.0",
    "font-awesome-webpack": "0.0.4",
    "less-loader": "^2.2.3",
    "mocha": "^2.5.3",
    "nock": "^8.0.0",
    "postcss-loader": "^0.8.1",
    "react-addons-test-utils": "^15.2.1",
    "react-hot-loader": "^1.3.0",
    "rucksack-css": "^0.8.5",
    "sass-loader": "^4.0.2",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.0",
    "stylus": "^0.54.5",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.7.1"
  },
  "dependencies": {
    "chai": "^3.5.0",
    "font-awesome": "^4.6.3",
    "node-sass": "^3.10.1",
    "normalize.css": "^5.0.0",
    "react": "^15.0.0",
    "react-addons-shallow-compare": "^15.3.2",
    "react-dom": "^15.0.0",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "react-router-redux": "^4.0.0",
    "redux": "^3.3.1",
    "redux-actions": "^0.9.1",
    "redux-mock-store": "^1.1.2",
    "redux-persist": "^3.2.2",
    "redux-saga": "^0.11.0",
    "stylus-loader": "^2.3.1"
  }
}
